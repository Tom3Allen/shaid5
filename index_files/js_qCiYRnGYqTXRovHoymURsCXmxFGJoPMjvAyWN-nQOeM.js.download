(function ($) {
  'use strict';

  /**
   * Custom video functions.
   * @type {{attach: Function}}
   */
  Drupal.behaviors.warchildVideo = {
    attach: function () {
      Drupal.behaviors.warchildVideo.ready = function (player_id) {

        function getBreakpoint() {
          // Get the breakpoint.
          var breakpoint = {
            name: 3
          };
          if (typeof Drupal.theme.prototype.warchildJSbreakpoints === 'function') {
            breakpoint = Drupal.theme.prototype.warchildJSbreakpoints();
          }
          return breakpoint;
        }

        function checkPlayer(player_id) {
          if (player_id !== null && typeof player_id === 'object') {
            return 'youtube';
          }
          else if (typeof player_id === 'string') {
            if (player_id.substr(0, 5) === 'vimeo') {
              return 'vimeo';
            }
            else {
              throw 'Unknown video type';
            }
          }
        }

        function correctID(player, player_id) {
          switch (player) {
            case 'vimeo':
              return '#' + player_id;
            break;
            case 'youtube':
              return '#' + arguments[1][1]['a']['id'];
            break;
          }
        }

        function findFakePlay(id) {
          return $(id)
              .parents('.field--paragraph-video-link')
              .siblings('.fake-play-button');
        }

        function findOverlay(id) {
          return $(id)
              .parents('.video-iframe')
              .siblings('.video-container');
        }

        /**
         * On play.
         */
        function onPlay(args, player_id) {
          var breakpoint = getBreakpoint();

          // Check if we have a player id already or get it out of the args object.
          player_id = player_id === undefined ? args : player_id;
          player_id = correctID(checkPlayer(player_id), player_id);

          if (breakpoint.name > 2) {
            findOverlay(player_id).hide();
          }

          // Always hide the fake play button on play.
          findFakePlay(player_id).hide();

        }

        /**
         * On pause.
         */
        function onPause(args, player_id) {

          // Check if we have a player id already or get it out of the args object.
          player_id = player_id === undefined ? args : player_id;
          player_id = correctID(checkPlayer(player_id), player_id);

          findOverlay(player_id).show();
          findFakePlay(player_id).show();
        }

        if (player_id.substr(0,5) == 'vimeo') {
          Drupal.behaviors.vimeo.addCallback('play', onPlay);
          Drupal.behaviors.vimeo.addCallback('pause', onPause);
        }
        else if (player_id.substr(0,2) == 'yt') {
          Drupal.behaviors.youtube.addCallback('play', onPlay);
          Drupal.behaviors.youtube.addCallback('pause', onPause);
        }
      };

      $('.close-button').click(function () {
          $(this).parent('.video-container').hide();
      })

    }

  };

  /**
   * Vimeo Embed.
   * @type {{attach: Function}}
   */
  Drupal.behaviors.vimeo = {

    attach: function () {
      var players = [],
        eventCallbacks = {
          play: [],
          playProgress: [],
          pause: [],
          seek: [],
          finish: []
        };

      /**
       * Register a callback, allows for multiple callbacks for each event.
       *
       * @param eventType
       * @param callback
       */
      Drupal.behaviors.vimeo.addCallback = function (eventType, callback) {
        if (typeof callback === 'function') {
          eventCallbacks[eventType][eventCallbacks[eventType].length] = callback;
        }
      };

      /**
