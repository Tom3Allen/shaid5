  /**
   * Youtube Embed.
   * @type {{attach: Function}}
   */
  Drupal.behaviors.youtube = {
    attach: function () {
      var players = [],
        timer,
        eventCallbacks = {
          play: [],
          playProgress: [],
          pause: [],
          seek: [],
          finish: []
        };

      /**
       * Once the youtube video is loaded then we can attach the events to the player
       *
       * @param player_id
       *   the player id e.g.: yt-xUJSWSTB8Qg
       */
      function ready(player_id) {
        // Add any init functions here. (Could refactor to be publish/subscribe)
        Drupal.behaviors.warchildVideo.ready(player_id);
      }

      /**
       * Register a callback, allows for multiple callbacks for each event.
       *
       * @param eventType
       * @param callback
       */
      Drupal.behaviors.youtube.addCallback = function (eventType, callback) {
        if (typeof callback === 'function') {
          eventCallbacks[eventType][eventCallbacks[eventType].length] = callback;
        }
      };

      /**
       * Dynamically execute registered call backs
       *
       * @param eventName
       * @param passedArgs
       */
      function callEvent(eventName, passedArgs) {
        if (!eventCallbacks[eventName]) {
          return;
        }
        var arrayLength = eventCallbacks[eventName].length;
        for (var i = 0; i < arrayLength; i++) {
          eventCallbacks[eventName][i].apply(null, passedArgs);
        }
      }

      // Wrapper functions for each event callback
      function onPlay() {
        callEvent('play', [arguments]);
      }

      function onPlayProgress() {
        callEvent('playProgress', arguments);
      }

      function onSeek() {
        callEvent('seek', arguments);
      }

      function onPause() {
        callEvent('pause', [arguments]);
      }

      function onFinish() {
        callEvent('finish', arguments);
      }

      function onReady(id) {
        return function(event) {
          ready(id);
        }
      }

      /**
       * onStateChange Handler to fire custom events.
       *
       * @param event
       */
      function onPlayerStateChange(event) {
        var player = event.target;
        var data = {
          seconds: player.getCurrentTime(),
          duration: player.getDuration()
        };

        // End onPlayProgress if not in playing state.
        if (event.data !== YT.PlayerState.PLAYING) {
          clearTimeout(timer);
        }
        // Fire events when player changes state.
        switch (event.data) {
          case YT.PlayerState.PLAYING:
            onPlay(data, player);
            // Set timer to replicate vimeo playProgress event.
            timer = setInterval(function() {
              data = {
                seconds: player.getCurrentTime(),
                duration: player.getDuration()
              };
              onPlayProgress(data, player);
            }, 500);
            break;
          case YT.PlayerState.ENDED:
            onFinish(data, player);
            break;
          case YT.PlayerState.PAUSED:
            onPause(data, player);
            break;
          default:
            onPlayProgress(data, player);
        }
      }

      // Initialize player for each iframe.
      window.onYouTubeIframeAPIReady = function() {
        $('iframe.paragraph-video-youtube').each(function() {
          var id = this.id;

          players[id] = new YT.Player(id, {
            events: {
              'onReady': onReady(id),
              'onStateChange': onPlayerStateChange
            }
          });

          // Add the play button.
          $(this)
              .parents('.field--paragraph-video-link')
              .siblings('.fake-play-button')
              .on('click', function() {
            players[id].playVideo();
          });

        });
      };
    }
  };

