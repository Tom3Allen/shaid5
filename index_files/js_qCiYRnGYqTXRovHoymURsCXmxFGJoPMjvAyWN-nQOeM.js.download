        }
        var arrayLength = eventCallbacks[eventName].length;
        for (var i = 0; i < arrayLength; i++) {
          eventCallbacks[eventName][i].apply(null, passedArgs);
        }
      }

      // Wrapper functions for each event callback
      function onPlay() {
        callEvent('play', [arguments, this.element.id]);
      }

      function onPlayProgress() {
        callEvent('playProgress', arguments);
      }

      function onSeek() {
        callEvent('seek', arguments);
      }

      function onPause() {
        callEvent('pause', [arguments, this.element.id]);
      }

      function onFinish() {
        callEvent('finish', arguments);
      }

      // Initialise each iframe
      $('iframe.paragraph-video-vimeo').each(function () {
        var id = this.id,
            player_id = $(this).attr('id');
        players[id] = new Vimeo.Player(this);
        players[id].on('play', onPlay);
        players[id].on('progress', onPlayProgress);
        players[id].on('seeked', onSeek);
        players[id].on('pause', onPause);
        players[id].on('ended', onFinish);
        Drupal.behaviors.warchildVideo.ready(id);

        // Add the play button.
        var playButton = $('#' + id).parents('.field--paragraph-video-link').siblings('.fake-play-button');
        playButton.on('click', function() {
