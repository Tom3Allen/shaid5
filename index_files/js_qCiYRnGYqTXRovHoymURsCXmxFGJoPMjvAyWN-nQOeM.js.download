(function ($) {
Drupal.behaviors.warchildTouchTriggers = {
  attach: function () {

    function is_touch_device() {
      return (('ontouchstart' in window)
      || (navigator.MaxTouchPoints > 0)
      || (navigator.msMaxTouchPoints > 0));
    }

    if (is_touch_device()) {
      $('body').addClass('js-is-touch-device');
    }

    $(document).click(function (e) {
      if ($(e.target).hasClass('group-cta-position-wrapper')) {
        // Reset all other CTA hover items.
        toggleHover('.cta-type-hover', 'div', 'removeClass', 'touched');
        toggleHover(findCorrectwrapper(e.target), 'div', 'addClass', 'touched');
      }
      else {
        toggleHover('.cta-type-hover', 'div', 'removeClass', 'touched');

      }
    })
  }
};

Drupal.behaviors.warchildCTAimageHover = {
  attach: function () {
    var cta_hover = $('.cta-type-hover');

    if ($('body').hasClass('js-is-touch-device') === false) {
      cta_hover.hover(
          function () {
            toggleHover(this, 'div', 'addClass', 'hovered');
          },
          function () {
            toggleHover(this, 'div', 'removeClass', 'hovered');
          })
    }
  }
};

  function findCorrectwrapper(that) {
    if ($(that).hasClass('.cta-type-hover')) {
      return $(that);
    }
    else if ($(that).parents('.cta-type-hover').length === 1) {
      return $(that).parents('.cta-type-hover');
    }
  }

  function toggleHover(that, selector, functionname, arg) {
    return $(that).find(selector)[functionname](arg);
  }

})(jQuery);;
