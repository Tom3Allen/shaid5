/**
 * @file warchild_donation_widget.js
 */

(function ($) {

  Drupal.behaviors.warchildDonationWidget = {
    attach: function (context) {

      // Prevent multiple loading of js when widget also appears in hero.
      if (context == document) {

        // Hide the block by default.
        $('.block--warchild-donation-widget').hide();

        // On click, either show the donate widget or hide it depending on where
        // the click originated from.
        $(document).click(function (event) {
          var breakpoint = Drupal.theme.prototype.warchildJSbreakpoints();

          var $target = $(event.target);
          var widget = '#block-warchild-donation-widget-warchild-donation-widget';

          if ($target.is('.block--system-main-menu > ul li.last a, .mobile-donate')) {
            event.preventDefault();
            if (breakpoint.name > 2 || $target.hasClass('mobile-donate')) {
              $(widget).slideToggle();
              $('.donation-widget__toggle .menu-icon').toggleClass('active');
            }
          }
          else {
            if ($target.is('.menu-icon, .donation-widget__toggle') ||
              $(widget).find($target).length === 0) {
              $(widget).slideUp();
              $('.donation-widget__toggle .menu-icon').removeClass('active');
            }
          }
        });

        // Show/hide text for regular radios (this is taken care of for select or
        // other fields in warchild_webform.js).
        showWidgetText($('.warchild-donation-widget-amount .form-radio:checked').not('.select-or-other-select'));
        $('.warchild-donation-widget-amount .form-radio').not('.select-or-other-select').click(function (event) {
          showWidgetText($(this));
        });

        function showWidgetText(element) {
          element.parents('form').find('div[class*=amount-text]').hide();
          var $selected = element.parents('form').find(".form-radio:checked").val();
          element.parents('form').find('div[class=amount-text-' + parseInt($selected) + ']').show();
        }

        // When the user clicks the donate button, ensure they have selected all
        // fields.
        $('.warchild-donation-widget-form .form-submit').click(function (event) {
          // Get the form.
          var currentForm = $(this).parents('.warchild-donation-widget-form');
          // Get the amount value chosen from the radios.
          var amountValue = currentForm.find('.form-item-amount .form-radio:checked').val();
          // If the select or other radio has been chosen, get the value entered.
          if (amountValue === 'select_or_other') {
            var amountValue = currentForm.find('.form-item-amount .form-text').val();
          }
          // Get the donation type field.
          var typeField = currentForm.find('input[name=type]');
          if (typeField.attr('type') === 'hidden') {
            // Get the value of the hidden field.
            var typeValue = typeField.val();
          }
          else {
            // Get the value of the selected radio.
            var typeValue = currentForm.find('.form-item-type .form-radio:checked').val();
          }
          // If both values have not been provided, prevent the submission.
          if (amountValue == null || typeValue == null) {
            event.preventDefault();
          }
        });
      }
    }
  };
})
